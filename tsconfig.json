{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "target": "ESNext",
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "@/utils/": ["src/utils/*"],
      "@/images/": ["src/images/*"],
      "@/styles/*": ["src/styles/*"],
      "@/components/*": ["src/components/*"]
    }
  },
  "include": ["src", "styles-system"]
}
